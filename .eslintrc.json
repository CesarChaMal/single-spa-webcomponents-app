{
  "root": true,
  "env": { "browser": true, "es6": true },
  "extends": ["eslint:recommended", "airbnb-base"],
  "rules": {
    "linebreak-style": 0,
    "import/no-cycle": 0,
    "import/extensions": 0,
    "class-methods-use-this": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}